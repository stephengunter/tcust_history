@model IEnumerable<TzuChiClassLibrary.BO.FileUploadModel>
<div class="Mv1">
    @try
    {
        if (Model != null && Model.Cast<Object>().Count() > 0 && Model.First() != null)
        {
            <video width="1150" height="850" autoplay controls poster="@Model.First().PreviewPath"></video>
			<!--video width="1150" height="850" autoplay controls poster="@Model.First().PreviewPath"></video-->
        }
        else
        {
            <video width="1150" height="850" autoplay controls poster="~/Images/TeachPhilosophy/withoutVideo.jpg"></video>
        }
    }
    catch (Exception e)
    {
        <text>
        @e.Message
        </text>
    }
</div>
<div class="Mvlist1" style="overflow-x:hidden;overflow-y:visible;">
    @if (Model != null)
    {
        foreach (var video in Model)
        {
            <ul class="Mvtxt"><a href="#">@video.FileName</a></ul>
            <ul class="Mvglow"><a href="#"><img src="@video.PreviewPath" data-video="@video.Path" width="235" height="165" border="0" /></a></ul>
        }
    }
</div>