<script type="text/javascript">
    function DropDown(el) {
        this.dd = el;
        this.initEvents();
    }
    DropDown.prototype = {
        initEvents: function () {
            var obj = this;

            obj.dd.on('click', function (event) {
                $(this).toggleClass('active');
                event.stopPropagation();
            });
        }
    }

    $(function () {

        var dd = new DropDown($('#dd'));

        $(document).click(function () {
            // all dropdowns
            $('.wrapper-dropdown-5').removeClass('active');
        });

    });

    $(document).ready(function () {
        $('body').css('top', -(document.documentElement.scrollTop) + 'px').addClass('noscroll');
            //翻頁特效
            $('.flipbook').turn({
                page: 2,
                width: 1500,
                height: 905,
                elevation: 0,
                gradients: true,
                autoCenter: false,
                when: {
                    start: function (event, pageObject, corner) {
                        if (pageObject.next == 1)
                            event.preventDefault();
                    },
                    turning: function (event, page, view) {
                        if (page == 1)
                            event.preventDefault();
                    }
                }
            });

            $('.dropdown li').click(function () {
                $('#dd div').text($(this).find('a').text());
                switch ($(this).index()) {
                    case 0:
                        $(".flipbook").turn("page", 2);
                        break;
                    case 1:
                        $(".flipbook").turn("page", 4);
                        break;
                    case 2:
                        $(".flipbook").turn("page", 6);
                        break;
                    case 3:
                        $(".flipbook").turn("page", 8);
                        break;
                    case 4:
                        $(".flipbook").turn("page", 12);
                        break;
                    case 5:
                        $(".flipbook").turn("page", 16);
                        break;
                    case 6:
                        $(".flipbook").turn("page", 22);
                        break;
                    case 7:
                        $(".flipbook").turn("page", 28);
                        break;
                    case 8:
                        $(".flipbook").turn("page", 38);
                        break;
                    case 9:
                        $(".flipbook").turn("page", 48);
                        break;
                    case 10:
                        $(".flipbook").turn("page", 58);
                        break;
                    case 11:
                        $(".flipbook").turn("page", 68);
                        break;
                    case 12:
                        $(".flipbook").turn("page", 78);
                        break;
                    case 13:
                        $(".flipbook").turn("page", 88);
                        break;
                    case 14:
                        $(".flipbook").turn("page", 100);
                        break;
                    case 15:
                        $(".flipbook").turn("page", 112);
                        break;
                    case 16:
                        $(".flipbook").turn("page", 122);
                        break;
                    case 17:
                        $(".flipbook").turn("page", 132);
                        break;
                    case 18:
                        $(".flipbook").turn("page", 144);
                        break;
                    case 19:
                        $(".flipbook").turn("page", 156);
                        break;
                    case 20:
                        $(".flipbook").turn("page", 170);
                        break;
                    case 21:
                        $(".flipbook").turn("page", 184);
                        break;
                    case 22:
                        $(".flipbook").turn("page", 198);
                        break;
                    case 23:
                        $(".flipbook").turn("page", 210);
                        break;
                    case 24:
                        $(".flipbook").turn("page", 224);
                        break;
                    case 25:
                        $(".flipbook").turn("page", 238);
                        break;
                    default:
                        break;
                }
            });
            if ($('#videobtn').length !== 0) {
                $('#videobtn').click(function () {
                    $('#MvDiv').show();
                    $('#MvDiv1').show();
                });
            }
    });
</script>

