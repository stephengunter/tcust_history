@{
    String video = "video.folder.MultielementAction.Video";
    String img = "video.folder.MultielementAction.Img";
}
<script type="text/javascript">
    $(document).ready(function () {
        $('#bookDiv').slick({
            //prevArrow: $('.arrowL'),
            //nextArrow: $('.arrowR'),
            centerMode: true,
            slidesToShow: 1,
            centerPadding: '300px',
            touchThreshold: 100,
            infinite: false,
            onAfterChange:adjustShadow,
            onInit:adjustShadow
        });

        if ($('#videobtn').length !== 0) {
            $('#videobtn').click(function () {
                $('#MvDiv').show();
                $('#MvDiv1').show();
            });
        }
    });


</script>
<style>
    .slick-slide:not(.slick-center) {
        -webkit-transform: scale(0.8);
        transform: scale(0.8);
        opacity: 0.5;
    }

    .slick-center {
        -webkit-transform: scale(1);
        transform: scale(1);
        opacity: 1;
    }

    #bookDiv {
        margin-top: 100px;
	height:800px;
    }
</style>

