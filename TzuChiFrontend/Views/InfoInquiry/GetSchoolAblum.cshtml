}

@section scripts {
    <script src="@Url.Content("~/Scripts/slick_v.js")"></script>
    <script>
        $('#slick-area').slick({
            centerMode: false,
            dots: false,
            vertical: true,
            slidesToShow: 1,
            infinite: true,
            arrows: true
        });

        $('.picmenu2').click(function () {
            $('.slick-prev').click();
        });

        $('.picmenu3').click(function () {
            $('.slick-next').click();
        });
    </script>
}

